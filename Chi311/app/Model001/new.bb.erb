<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Model001</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @model001.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Service request: </td>
                <td class="itemValue"><input type="text" name="model001[service_request_id]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Requested datetime: </td>
                <td class="itemValue"><input type="text" name="model001[requested_datetime]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Address: </td>
                <td class="itemValue"><input type="text" name="model001[address]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
